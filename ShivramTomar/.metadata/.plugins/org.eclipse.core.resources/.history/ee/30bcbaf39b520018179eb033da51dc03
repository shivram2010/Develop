	@CucumberOptions(strict = true,
	monochrome = true,
	features = {"src/test/resources/features"}
					 	plugin = {"pretty:target/cucumber-pretty.txt",
	        					"usage:target/cucumber-usage.json", 
	        					"html:target/cucumber-pretty",
					 		    "json:target/cucumber.json"}, glue="")
public class Runner {

}
